<!DOCTYPE html>
<html>
  <head>
    <title>FitPlanningPartner</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  
<div>
   <a class="navbar-brand" href="<%= root_path %>"/>Home</a>
        <a class="navbar-brand" href="<%= new_user_path %>"/> Sign Up</a>
        <a class="navbar-brand" href="<%= '/login' %>"/> Login</a>
        <% if logged_in? %>
          <a class="navbar-brand" href="<%= new_exercise_path %>"/> New Exercise</a>
          <a class="navbar-brand" href="<%= exercises_path %>"/> Exercises</a>
          <a class="navbar-brand" href="<%= new_workout_path %>"/> New Workout</a>
          <a class="navbar-brand" href="<%= user_workouts_path(current_user.id) %>"/> My Workouts </a>
          <a class="navbar-brand" <%= link_to "Log Out", logout_path, method: 'delete' %></a>
        <% end %>
  </div>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
