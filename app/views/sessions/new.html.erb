<h1> Log in to continue! </h1>

<h2> Already have an account? </h2>
<%= form_for :user, url: login_path do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Log In" %>
<% end %>
<br>
<h2>Or log in with your Google account </h2>
<%= button_to "Log in with Google", '/auth/google_oauth2'  %>
