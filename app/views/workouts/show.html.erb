<h1> <%= current_user.name %>'s <%= @workout.name %> Workout </h1>

<ul> 
    <% @workout.routines.each do |w| %>
    <li>
    <%= w.exercise.name %> <%= w.sets %> X <%= w.reps %> <br>
    </li>
    <% end %>
    <br>
    <%= link_to "Add exercise routine", new_workout_routine_path(@workout) %><br><br>
    <%= link_to "Delete", workout_path(@workout), method: 'delete' %>


</ul>