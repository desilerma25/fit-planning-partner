<h1> <%= @workout.name %> Workout </h1>

<ul> 
    <% @workout.routines.each do |w| %>
    <li>
    <%= w.exercise.name %> <%= w.sets %> X <%= w.reps %> <br>
    </li><br>
    <% end %>
    <br>

    <% if authorized_workout_user %>
    <%= link_to "Edit Workout", edit_workout_path %>
    <%= link_to "Add Another Exercise", new_workout_routine_path(@workout) %><br><br>
    <%= link_to "Delete", workout_path(@workout), method: 'delete' %>
    <% end %>
</ul>